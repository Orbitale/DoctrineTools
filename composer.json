{
    "name": "orbitale/doctrine-tools",
    "description": "A pack of tools to use with Doctrine ORM",
    "keywords": ["doctrine", "orm", "tools", "repository", "fixtures"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Orbitale/DoctrineTools",
    "authors": [
        {
            "name": "Alex Rock Ancelet",
            "email": "alex@orbitale.io",
            "homepage": "http://www.orbitale.io/"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2.6",
        "doctrine/instantiator": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "doctrine/data-fixtures": "^1.0"
    },
    "suggest": {
        "doctrine/data-fixtures": "To use the nice AbstractFixture class",
        "doctrine/doctrine-fixtures-bundle": "To use the AbstractFixture class inside a Symfony app"
    },
    "autoload": {
        "psr-4": {
            "Orbitale\\Component\\DoctrineTools\\" : ""
        }
    }
}
